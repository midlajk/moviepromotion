<%- include('../admininclude/head.ejs') %>
    <div class="container-fluid">

        <!-- Page Heading -->


        <!-- DataTales Example -->
        <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-primary">Previous Promotiona Messages
                </h6>

            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th>Date & Time</th>
                                <th>Message send</th>
                            </tr>
                        </thead>
                        <tfoot>
                            <tr>
                                <th>Date & Time</th>
                                <th>Message send</th>



                            </tr>
                        </tfoot>
                        <tbody>

                            <%for(let doc of docs){%>
                                <tr>
                                    <td>

                                        <%=doc.date.toLocaleString()%>

                                    </td>
                                    <td>
                                        <%=doc.message%>
                                    </td>




                                </tr>
                                <%}%>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>


    <%- include('../admininclude/end.ejs') %>